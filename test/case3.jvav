int n;
int m;
int result;

// 自定义一个函数
int add(int a, int b) {
    return a + b;
}

int main() {
    n = getInt();
    m = getInt();
    
    result = add(n, m);
    
    printIntln(result);
    
    // 判断奇偶
    if (result % 2 == 0) {
        print("even\n");
    } else {
        print("odd\n");
    }
    
    // for循环输出 1 ~ n
    int i;
    for (i = 1; i <= n; i++) {
        printInt(i);
        print(" ");
    }
    
    // while 循环输出 n 的阶乘
    int factorial;
    factorial = 1;
    i = 1;
    while (i <= n) {
        factorial = factorial * i;
        i++;
    }
    
    printInt(factorial);
    
    return 0;
}
